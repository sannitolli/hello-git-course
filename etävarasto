git remote add intermediate-branches https://course-gitlab.tuni.fi/git-course/intermediate-branches.git
git pull --allow-unrelated-histories intermediate-branches main
git checkout -b feature/create-awesome
echo 'print("Hello from feature")' >> hello_world.py
git add hello_world.py
git commit -m "Add feature print statement"
git push -u origin feature/create-awesome
git checkout -b release feature/create-awesome
echo 'print("Hello from release")' >> hello_world.py
git add hello_world.py
git commit -m "Add release print statement"
git push -u origin release
